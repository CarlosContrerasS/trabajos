package Prueba;

public class Caguano extends Carro{
	private int alcanceTiro;
	private String colorConfeti;
	private int [][] Matriz_caguano= new int[1][2]; //para la posicion en el tablero
	
	public Caguano(int cantOcupantes, int fechaIng, int[] posicion, int alcanceTiro, String colorConfeti) {
		super(cantOcupantes, fechaIng, posicion);
		this.alcanceTiro = alcanceTiro;
		this.colorConfeti = colorConfeti;
	}
	public Caguano(){
		
	}
	
	public int getAlcanceTiro() {
		return alcanceTiro;
	}
	public void setAlcanceTiro(int alcanceTiro) {
		this.alcanceTiro = alcanceTiro;
	}
	public String getColorConfeti() {
		return colorConfeti;
	}
	public void setColorConfeti(String colorConfeti) {
		this.colorConfeti = colorConfeti;
	}
	
}
