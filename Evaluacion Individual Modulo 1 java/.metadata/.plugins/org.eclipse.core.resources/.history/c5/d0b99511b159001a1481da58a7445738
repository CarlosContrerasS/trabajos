 */
public class MainElectrodomesticos {

	
	public static void main(String[] args) {
		
		Electrodomesticos[] ArrayElec_1 = new Electrodomesticos[10];
		
		//Debemos Recordar como fueron creados los constructores.
		
		// Las posibilidades de color son: blanco, negro, rojo, azul y gris.
		// Las posibilidades de Consumo Energetico Son: "A", "B", "C", "D", "E" y"F"
		
		//Electrodomesticos precio, color, ConsumoEnergetico y peso
		ArrayElec_1 [0] = new Electrodomesticos(50, "rojo",'A', 30);
		
		ArrayElec_1 [1] = new Electrodomesticos(250, "negro",'B', 80);
		ArrayElec_1 [2] = new Electrodomesticos(150, "blanco",'D', 100);
		
		
		//Television resolucion, sintonizador, precio, color, consumoEnergetico y  peso
		
		ArrayElec_1 [7] = new Television(40, true, 150, "rojo",'A', 60);
		ArrayElec_1 [8] = new Television(40, true, 150, "rojo",'A', 60);
		ArrayElec_1 [9] = new Television(40, true, 150, "rojo",'A', 60);
		
		
		
		
		//Lavadora precio, color, ConsumoEnergetico y peso)
		ArrayElec_1 [3] = new Lavadora (150, "rojo",'A', 60);
		ArrayElec_1 [4] = new Lavadora(150, "rojo",'A', 60);
		ArrayElec_1 [5] = new Lavadora(150, "rojo",'A', 60);
		ArrayElec_1 [6] = new Lavadora(150, "rojo",'A', 60);
		
		
		
		
		
		
		
		System.out.println(ArrayElec_1[9].getPrecio());
	}

}
